gabarito=input().split()
n=int(input())
acertos=[]
i=0
contador=0
while i<n:
    resposta=input().split()
    contador=0
    i2=0
    while i2<len(resposta):
        if resposta[i2]==gabarito[i2]:
            contador+=1
        i2+=1
    acertos.append(contador)
    i+=1
i=0
while i<len(acertos):
    print(acertos[i])
    i+=1